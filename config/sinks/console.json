{
  "id": "console",
  "type": "console",
  "name": "Console Output",
  "enabled": true,
  "config": {},
  "template": {
    "title": "[{{server.displayName}}] {{event.name}}",
    "body": "{{message.content}}",
    "format": "text"
  },
  "allowedMetadata": [
    "title",
    "body",
    "format",
    "color"
  ],
  "metadata": {
    "description": "Output notifications to console. Supports custom title, body, format (text/json), and color via event metadata."
  }
}