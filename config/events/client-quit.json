{
  "id": "client-quit",
  "name": "You Disconnected",
  "enabled": true,
  "baseEvent": "quit",
  "serverIds": [
    "redacted"
  ],
  "filters": {
    "operator": "AND",
    "filters": [
      {
        "field": "sender.nickname",
        "operator": "equals",
        "value": "{{server.metadata.clientNickname}}"
      }
    ]
  },
  "sinkIds": [
    "ntfy"
  ],
  "priority": 400,
  "metadata": {
    "description": "Triggers when the IRC client itself disconnects from a server",
    "sink": {
      "ntfy": {
        "priority": "high",
        "tags": [
          "red_circle",
          "broken_heart",
          "warning"
        ]
      }
    }
  },
  "__originalId": "client-quit",
  "group": "Generic"
}